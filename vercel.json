{
  "buildCommand": "bash build.sh",
  "devCommand": "python api/index.py",
  "runtime": "python@3.9",
  "functions": {
    "api/**/*.py": {
      "runtime": "python@3.9",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ]
}